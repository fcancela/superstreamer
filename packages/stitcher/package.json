{
  "name": "@superstreamer/stitcher",
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "bun --watch ./src/index.ts",
    "build": "tsc -b && bun build ./src/index.ts --target=bun --outdir=./dist",
    "test": "bun test",
    "lint": "eslint",
    "dev:cloudflare": "wrangler dev src/serverless/cloudflare.ts",
    "deploy:cloudflare": "wrangler deploy --minify src/serverless/cloudflare.ts"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@types/bun": "latest",
    "@types/hh-mm-ss": "^1.2.3",
    "@types/lru-cache": "^7.10.10",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.1.0",
    "@types/uuid": "^10.0.0",
    "config": "workspace:*",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "wrangler": "^3.83.0"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.1",
    "@xmldom/xmldom": "^0.8.10",
    "dom-parser": "^1.1.5",
    "elysia": "^1.1.20",
    "hh-mm-ss": "^1.2.0",
    "lru-cache": "^11.0.1",
    "luxon": "^3.5.0",
    "redis": "^4.7.0",
    "shared": "workspace:*",
    "uuid": "^10.0.0",
    "vast-client": "workspace:*",
    "@superstreamer/api": "workspace:*"
  }
}
